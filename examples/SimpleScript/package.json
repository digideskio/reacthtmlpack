{
  "name": "SimpleScript",
  "version": "1.0.0",
  "description": "example of showing simple script entry point using reacthtmlpack",
  "scripts": {
    "clean": "rimraf ../../public/index.html ../../public/assets",
    "predev": "npm run clean",
    "dev": "NODE_ENV=development reacthtmlpack buildToDir ../../public './views/*.html.js'"
  },
  "devDependencies": {
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "rimraf": "^2.4.3",
    "webpack": "^1.12.2"
  },
  "dependencies": {
    "node-libs-browser": "^0.5.3",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-google-maps": "^4.0.0"
  }
}
